{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "group:all"
  ],
  // make sure automerge is disabled, even when base configs change
  "automerge": false,
  "dependencyDashboard": false,
  "lockFileMaintenance": {
    "enabled": true
  },
  "includePaths": [
    ".github/**"
  ],
}
